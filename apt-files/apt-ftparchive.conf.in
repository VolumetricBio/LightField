Dir {
    ArchiveDir          "@@REPO_DIR@@";
    CacheDir            "@@APT_CACHE_DIR@@";
};

Default {
    Packages::Compress  "xz";
    Sources::Compress   "xz";
    Contents::Compress  "xz";
};

Tree "dists/$(DIST)" {
    Sections "main";
    Architectures "amd64 all";
};

BinDirectory "pool/main" {
    Packages "dists/$(DIST)/main/binary-$(ARCH)/Packages";
    Contents "dists/$(DIST)/main/Contents-$(ARCH)";
}
